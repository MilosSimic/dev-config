[
    { "key": "alt+up",
    "command": "cursorHome",
    "when": "editorTextFocus" 
    },
    { "key": "alt+down",
    "command": "cursorEnd",
    "when": "editorTextFocus"
    },
    { "key": "shift+alt+up",
    "command": "cursorHomeSelect",
    "when": "editorTextFocus"
    },
    { "key": "shift+alt+down",
    "command": "cursorEndSelect",
    "when": "editorTextFocus" 
    },
    { "key": "ctrl+w",
    "command": "deleteWordLeft",
    "when": "editorTextFocus" 
    }
]
